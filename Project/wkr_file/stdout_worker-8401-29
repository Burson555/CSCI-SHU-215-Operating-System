rm -rf bin obj opf
mkdir obj
gcc -Iinclude -Wall -c -o obj/policies.o src/sched-policies.c	
gcc -Iinclude -Wall -c -o obj/simulator.o src/sched-simulator.c
mkdir bin
mkdir opf
gcc -Iinclude -Wall -o bin/sched-simulator obj/simulator.o obj/policies.o
bin/sched-simulator tsk/tasks1 RR 4 > opf/sched-runR1.txt
cat  opf/sched-runR1.txt
